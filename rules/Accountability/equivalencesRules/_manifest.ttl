                PREFIX dce: <http://purl.org/dc/elements/1.1/>
                PREFIX dcterms: <http://purl.org/dc/terms/>
                INSERT {
                    GRAPH ?kgGraph {
                        ?kg dcterms:modified ?date .
                        ?kg schema:dateModified ?date .
                        ?kg pav:lastUpdateOn ?date .
                        ?kg pav:contributedOn ?date .
                        # With PROV
                        ?kg prov:wasGeneratedBy [
                            a ?activityType ;
                            ?dateProperty ?date
                        ]
                    }
                } WHERE {
                    GRAPH ?kgGraph {
                        ?kg a dcat:Dataset .
