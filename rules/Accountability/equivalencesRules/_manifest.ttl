                INSERT {
                    GRAPH ?kgGraph {
                        ?kg dcterms:issued ?date .
                        ?kg dcterms:available ?date .
                        ?kg schema:datePublished ?date .
                        ?kg schema:sdDatePublished ?date .
                        ?kg prov:wasGeneratedBy [
                            a prov:Publish ;
                            prov:endedAtTime ?date
                        ]
                    }
