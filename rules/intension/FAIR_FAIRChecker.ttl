# F2A strong
# ?kg a dcat:Dataset .
# dct:title
# dct:description
# dcat:accessURL
# dcat:downloadURL
# dcat:endpointDescription
# dcat:endpointURL
kgi:F2AstrongFAIRDataset a rdf:Class ;
    rdfs:label "F2B strong FAIR Dataset" ;
    rdfs:comment "A dataset that is follows the FAIR principles and uses Structured metadata" ;
    rdfs:isDefinedBy kgi: ;
    owl:equivalentClass [
        rdf:type owl:Class ;
        owl:unionOf (
            kgi:DatasetWithDescriptionAnnotation
            kgi:DatasetWithDumpAnnotation
            kgi:DatasetWithSparqlEndpointAnnotation
            kgi:DatasetWithTitleAnnotation
            [
                owl:intersectionOf (
                    [
                        rdf:type owl:Class ;
                        owl:unionOf (
                            dcat:Dataset
                            schema:Dataset
                            dcmitype:Dataset
                            void:Dataset
                            sd:Dataset
                        )
                    ]
                    [
                        rdf:type owl:Class ;
                        owl:unionOf (
                            [
                                rdf:type owl:Restriction ;
                                owl:onProperty dcat:accessURL ;
                                owl:someValuesFrom rdfs:Resource
                            ]
                            [
                                rdf:type owl:Restriction ;
                                owl:onProperty dcat:downloadURL ;
                                owl:someValuesFrom rdfs:Resource
                            ]
                            [
                                rdf:type owl:Restriction ;
                                owl:onProperty dcat:endpointDescription ;
                                owl:someValuesFrom rdfs:Resource
                            ]
                            [
                                rdf:type owl:Restriction ;
                                owl:onProperty dcat:endpointURL ;
                                owl:someValuesFrom rdfs:Resource
                            ]
                        )
                    ]
                )
            ]
        )
    ] .

