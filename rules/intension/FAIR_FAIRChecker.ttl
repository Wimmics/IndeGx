kgi:PartiallyFAIRDataset rdf:type rdf:Class ;
    rdfs:label "Partially FAIR dataset" ;
    rdfs:comment "Dataset that follows rdf:type part of the FAIR principles." ;
    owl:equivalentClass [
        rdf:type owl:Class ;
        owl:unionOf (
            kgi:FindableFAIRDataset
            kgi:AccessibleFAIRDataset
            kgi:InteroperableFAIRDataset
            kgi:ReusableFAIRDataset
        )
    ] .

kgi:FAIRDataset rdf:type rdf:Class ;
    rdfs:subClassOf kgi:PartiallyFAIRDataset ;
    rdfs:label "FAIR Dataset" ;
    rdfs:comment "A dataset that is follows the FAIR principles" ;
    owl:equivalentClass [
        rdf:type owl:Class ;
        owl:intersectionOf (
            kgi:FindableFAIRDataset
            kgi:AccessibleFAIRDataset
            kgi:InteroperableFAIRDataset
            kgi:ReusableFAIRDataset
        )
    ] .

kgi:FindableFAIRDataset rdf:type rdf:Class ;
    rdfs:label "Findable dataset"@en ;
    rdfs:comment "Dataset that follows the principle of Findability of the FAIR principles"@en ;
    owl:intersectionOf (
        kgi:F1AFAIRDataset
        kgi:F1BFAIRDataset
        kgi:F2AFAIRDataset
        kgi:F2BFAIRDataset
    ) .

kgi:AccessibleFAIRDataset rdf:type rdf:Class ;
    rdfs:label "Accessible dataset"@en ;
    rdfs:comment "Dataset that follows the principle of Accessibility of the FAIR principles"@en ;
