kgi:FAIRDataset a rdf:Class ;
    rdfs:label "FAIR Dataset" ;
    rdfs:comment "A dataset that is follows the FAIR principles" .

kgi:F1AFAIRDataset a rdf:Class ;
    rdfs:label "F1A FAIR Dataset" ;
    rdfs:comment "A dataset that is follows the FAIR principles and uses Unique IDs" ;
    rdfs:isDefinedBy kgi: ;
    owl:equivalentClass [
        rdf:type owl:Class ;
        owl:intersectionOf (
            [
                rdf:type owl:Class ;
                owl:unionOf (
                    dcat:Dataset
                    schema:Dataset
                    dcmitype:Dataset
                    void:Dataset
                    sd:Dataset
                )
            ]
        )
    ] .

