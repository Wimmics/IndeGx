# R1.2
# prov:wasGeneratedBy
# prov:wasDerivedFrom
# prov:wasAttributedTo
# prov:used
# prov:wasInformedBy
# prov:wasAssociatedWith
# prov:startedAtTime
# prov:endedAtTime
# dct:hasVersion
# dct:isVersionOf
# dct:creator
# dct:contributor
# dct:publisher
# pav:hasVersion
# pav:version
# pav:hasCurrentVersion
# pav:createdBy
# pav:authoredBy
# pav:retrievedFrom
# pav:importedFrom
# pav:createdWith
# pav:retrievedBy
# pav:importedBy
# pav:curatedBy
# pav:createdAt
# pav:previousVersion
# schema:creator
# schema:author
# schema:publisher
# schema:provider
# schema:funder
kgi:R12FAIRDataset rdf:type rdf:Class ;
    rdfs:label "R1.2 FAIR Dataset" ;
    rdfs:comment "A dataset that is follows the FAIR principles and uses Metadata includes provenance" ;
    rdfs:isDefinedBy kgi: ;
    owl:equivalentClass [
        owl:unionOf (
            kgi:DatasetWithAvailabilityStartAnnotation
            kgi:DatasetWithVersionAnnotation
            kgi:DatasetWithAuthorAnnotation
            kgi:DatasetWithContributorAnnotation
            kgi:DatasetWithPublisherAnnotation
            kgi:DatasetWithPROVAnnotation
            kgi:DatasetWithPAVAnnotation
            [
                rdf:type owl:Class ;
                owl:intersectionOf (
                    kgi:Dataset
                    [
                        rdf:type owl:Class ;
                        owl:unionOf (
                            [
                                rdf:type owl:Restriction ;
                                owl:onProperty dcterms:isVersionOf ;
                                owl:someValuesFrom rdfs:Resource
                            ]
                            [
                                rdf:type owl:Restriction ;
                                owl:onProperty schema:provider ;
                                owl:someValuesFrom rdfs:Resource
                            ]
                            [
                                rdf:type owl:Restriction ;
                                owl:onProperty schema:funder ;
                                owl:someValuesFrom rdfs:Resource
                            ]
                        )
                    ]
                )
            ]
        )
    ] .

