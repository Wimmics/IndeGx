                        rdf:type owl:Restriction ;
                        owl:onProperty schema:award ;
                        owl:someValuesFrom owl:Thing
                    ]
                    [
                        rdf:type owl:Restriction ;
                        owl:onProperty schema:award ;
                        owl:someValuesFrom rdfs:Literal
                    ]
                )
            ] 
        )
    ] ,
    # Equivalence with having a successful quality measurement of the usage/What/quality metric
    # ?kg dqv:hasQualityMeasurement ?qualityMeasurementQuality .
    # ?qualityMeasurementQuality a dqv:QualityMeasurement ;
    #     dqv:isMeasurementOf kgi:datasetUsageQuality ;
    #     dqv:value "true"^^xsd:boolean .
    [ 
        rdf:type owl:Class ;
        owl:intersectionOf (
            [
                rdf:type owl:Class ;
                owl:unionOf (
                    dcat:Dataset
                    schema:Dataset
                    dcmitype:Dataset
                    void:Dataset
                    sd:Dataset
                )
            ]
            [
                rdf:type owl:Restriction ;
                owl:onProperty dqv:hasQualityMeasurement ;
                owl:someValuesFrom [
                    rdf:type owl:Class ;
                    owl:intersectionOf (
                        [
                            rdf:type owl:Restriction ;
                            owl:onProperty dqv:isMeasurementOf ;
                            owl:hasValue kgi:datasetUsageQuality
                        ]
                        [
                            rdf:type owl:Restriction ;
                            owl:onProperty dqv:value ;
                            owl:hasValue "true"^^xsd:boolean
                        ]
                    )
                ]
            ]
        )
    ] .