kgi:commonDatatypeDescription rdf:type owl:ObjectProperty ;
    rdfs:label "common datatypes"@en ;
    rdfs:comment "Links a dataset to its datatype commonalities descriptions."@en ;
    rdfs:domain void:Dataset ;
    rdfs:range kgi:DatatypeCommonalityDescription ;
