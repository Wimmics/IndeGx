kgi:property rdf:type owl:ObjectProperty ;
    rdfs:label "property"@en ;
    rdfs:comment "Indicates the properties that are common to a group of datasets in an instance of CommonalityDescription."@en ;
    rdfs:domain kgi:PropertyCommonalityDescription ;
