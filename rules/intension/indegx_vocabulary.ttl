kgi: rdf:type owl:Ontology, voaf:Vocabulary ;
    rdfs:label "IndeGx vocabulary"@en ;
    dcterms:creator "Pierre Maillot", <https://orcid.org/0000-0002-9814-439X> ;
    dcterms:publisher "INRIA, Team WIMMICS", <https://www.inria.fr/fr/wimmics> ;
    dcterms:title  "IndeGx vocabulary"@en ;
    dcterms:description "Vocabulary extending the manifest and EARL vocabularies for the declaration of the IndeGx framework and the content of an index generated by the IndeGx framework"@en ;
    dcterms:date "2021-10" ;
    pav:lastUpdateOn "2023-06-20" ;
    dcterms:format "RDF" ;
    dcterms:identifier kgi: ;
    voaf:extends void: , mf: , earl: ;
    voaf:generalizes mf: , earl: ;
    # owl:imports earl: , mf: , dcat: , void: , sd: 
    .

# Index structuration vocabulary

kgi:IndeGxDataset rdf:type dcat:Dataset, schema:Dataset, void:Dataset, prov:Entity, sd:Dataset ;
    rdfs:label "IndeGx dataset root" ;
    rdfs:isDefinedBy kgi: ;
    rdfs:comment "Root of a dataset generated by IndeGx rules. Default resource used to represent the catalog at the root of the index."@en .

kgi:original rdf:type owl:ObjectProperty ;
    rdfs:label "original"@en ;
    rdfs:comment "Original description graph. Used to link the description of the indexation to the named graph containing the original data directly extracted from an endpoint by IndeGx"@en ;
    rdfs:isDefinedBy kgi: ;
    rdfs:domain prov:Activity ;
    rdfs:range sd:NamedGraph .

kgi:originalResource rdf:type owl:ObjectProperty ;
    rdfs:label "original resource"@en ;
    rdfs:comment "Original description resource. Resource describing a dataset of the endpoint directly extracted from the endpoint."@en ;
    rdfs:isDefinedBy kgi: ;
    rdfs:domain prov:Activity ;
    rdfs:range prov:Entity .

kgi:curated rdf:type owl:ObjectProperty ;
    rdfs:label "curated"@en ;
    rdfs:comment "Generated description graph. Used to link the description of the indexation to the named graph contaning the curated data generated by IndeGx."@en ;
    rdfs:isDefinedBy kgi: ;
    rdfs:domain prov:Activity ;
    rdfs:range sd:NamedGraph .

kgi:curatedResource rdf:type owl:ObjectProperty ;
    rdfs:label "curated resource"@en ;
    rdfs:comment "Generated description resource. Resource generated from the application of generation assets by IndeGx but not directly extracted from an endpoint."@en ;
    rdfs:isDefinedBy kgi: ;
    rdfs:domain prov:Activity ;
    rdfs:range prov:Entity .

kgi:summary rdf:type owl:ObjectProperty ;
    rdfs:label "summary"@en ;
    rdf:comment "Link between the indexation activity of an endpoint and the graph containing the summary of its content."@en ;
    rdfs:domain void:Dataset ;
    rdfs:range sd:NamedGraph .

kgi:trace rdf:type owl:ObjectProperty ;
    rdfs:label "trace"@en ;
    rdfs:comment "Trace report of the metadata generation. EARL report resulting in the execution of a query during the generation of the metadata."@en ;
    rdfs:isDefinedBy kgi: ;
    rdfs:domain prov:Entity ;
    rdfs:range earl:Assertion .

# Generation assets structuration vocabulary

kgi:endpoint rdf:type owl:ObjectProperty ;
    rdfs:label "endpoint"@en ;
    rdfs:comment "Indicates the endpoint to send a test or action."@en ;
    rdfs:domain [ owl:unionOf ( kgi:TestQuery kgi:Action ) ] ;
    rdfs:isDefinedBy kgi: .

kgi:TestQuery rdf:type rdfs:Class ;
    rdfs:label "Query test"@en ;
    rdfs:comment "Test based on a SPARQL query execution. Test the result of the execution of rdf:type query in the manner of rdf:type SHACL shape but allows declaration of constraints on the HTTP response and the result."@en ;
    rdfs:isDefinedBy kgi: ;
    rdfs:subClassOf earl:TestCase .

kgi:onSuccess rdf:type owl:ObjectProperty ;
    rdfs:label "on success"@en ;
    rdfs:comment "List of actions triggered by the success of the test present at the URI of the entry."@en ;
    rdfs:isDefinedBy kgi: ;
    rdfs:domain mf:ManifestEntry ;
    rdfs:range rdf:List .

kgi:onFailure rdf:type owl:ObjectProperty ;
    rdfs:label "on failure"@en ;
    rdfs:comment "List of actions triggered by the failure of the test present at the URI of the entry."@en ;
    rdfs:isDefinedBy kgi: ;
    rdfs:domain mf:ManifestEntry ;
    rdfs:range rdf:List .

kgi:DummyTest rdf:type rdfs:Class ;
    rdfs:label "Placeholder test"@en ;
    rdfs:isDefinedBy kgi: ;
    rdfs:comment "Placeholder class for the test of a rule only affecting the index data. Some tests and actions are 'meta' rules that do not need interaction with remote endpoints. This class is for a test that always succeed for these rules."@en ;
    rdfs:subClassOf earl:TestCase .

kgi:GenerationAsset rdf:type rdfs:Class ;
    rdfs:label "Generation asset."@en ;
    rdfs:isDefinedBy kgi: ;
    rdfs:comment "A generation asset is a test or an action that can be applied by IndeGx to a dataset to generate its description."@en .

kgi:requiredAssets rdf:type owl:ObjectProperty ;
    rdfs:label "required assets"@en ;
    rdfs:comment "Generation assets that must be applied for this generation asset to be applied."@en ;
    rdfs:isDefinedBy kgi: ;
    rdfs:domain kgi:GenerationAsset ;
    rdfs:range rdf:List .

kgi:Action rdf:type owl:Class ;
    rdfs:subClassOf kgi:GenerationAsset ;
    rdfs:label "Action"@en ;
    rdfs:comment "Action object. Must be linked with the property kgi:action to a SPARQL query"@en .

# Modifying existing vocabularies

earl:TestCase rdfs:subClassOf kgi:GenerationAsset .

mf:ManifestEntry rdfs:subClassOf kgi:GenerationAsset .

mf:Manifest rdfs:subClassOf kgi:GenerationAsset .

mf:action rdfs:domain kgi:Action .

# properties to set up the parameters of the indexation

kgi:timeout rdf:type owl:DatatypeProperty ;
    rdfs:label "timeout"@en ;
    rdfs:isDefinedBy kgi: ;
    rdfs:comment "Specify a timeout for a test or action. Queries sent during test and actions may need a specific timeout, generally longer than normal. This property specify the timeout for a test or an action."@en ;
    rdfs:domain kgi:Action ;
    rdfs:range xsd:duration .

kgi:recommendedPagination rdf:type owl:DatatypeProperty ;
    rdfs:label "recommended pagination"@en ;
    rdfs:comment "Recommended size for pagination during retrieval of the results of an action. This indicates the size of the page to use in the actions queries."@en ;
    rdfs:isDefinedBy kgi: ;
    rdfs:domain kgi:Action ;
    rdfs:range xsd:integer .

# Trace of indexation

kgi:sentQuery rdf:type owl:DatatypeProperty ;
    rdfs:comment "Query sent to the server. String representation of the SPARQL query sent to the server after transformation to adapt it to the endpoint, if necessary."@en ;
    rdfs:label "query sent"@en ;
    rdfs:domain earl:TestResult ;
    rdfs:range xsd:string .

# Pseudo-partitions and linksets

kgi:linkset rdf:type owl:ObjectProperty ;
    rdfs:label "linkset"@en ;
    rdfs:comment "Link a dataset description to the linkset that were retrieved during its indexation"@en ;
    rdfs:isDefinedBy kgi: ;
    rdfs:domain void:Dataset ;
    rdfs:range void:Linkset .

kgi:resourceHostname rdf:type owl:ObjectProperty ;
    rdfs:label "resource hostname"@en ;
    rdfs:comment "Hostname of the resources of the dataset. Linked to URIs representing the hostnames of the resources of the dataset."@en ;
    rdfs:isDefinedBy kgi: ;
    rdfs:domain void:Dataset .

kgi:hostnamePartition rdf:type owl:ObjectProperty ;
    rdfs:label "hostname partition"@en ;
    rdfs:comment "Partition of the dataset according to the hostname used the URIs of instances. This property is to be used on the same model as void:classPartition and void:propertyPartition."@en ;
    rdfs:subPropertyOf void:subset ;
    rdfs:isDefinedBy kgi: ;
    rdfs:domain void:Dataset .

kgi:vocabularyPartition rdf:type owl:ObjectProperty ;
    rdfs:label "vocabulary partition"@en ;
    rdfs:comment "Partition of the dataset according to the vocabulary used in triples. This property is to be used on the same model as void:classPartition and void:propertyPartition."@en ;
    rdfs:subPropertyOf void:subset ;
    rdfs:isDefinedBy kgi: ;
    rdfs:domain void:Dataset .

# Other dataset description properties

kgi:availableLiteralDatatypes rdf:type owl:ObjectProperty ;
    rdfs:label "available literal datatypes"@en ;
    rdfs:comment "Used to describe the datatype of the literals in the dataset."@en ;
    rdfs:isDefinedBy kgi: ;
    rdfs:domain void:Dataset ;
    rdfs:range xsd:string .

kgi:resourceHostname rdf:type owl:ObjectProperty ;
    rdfs:label "resource hostname"@en ;
    rdfs:comment "Used to describe the hostname of the resources of the dataset."@en ;
    rdfs:domain void:Dataset ;
    rdfs:isDefinedBy kgi: .

kgi:summary rdf:type owl:ObjectProperty ;
    rdfs:label "summary"@en ;
    rdfs:comment "Link between a dataset and a summary of its content."@en ;
    rdfs:domain void:Dataset ;
    rdfs:range sd:NamedGraph .

kgi:graph rdf:type owl:ObjectProperty ;
    rdfs:label "graph"@en ;
    rdfs:comment "Link between a dataset and a graph containing its content."@en ;
    rdfs:domain void:Dataset ;
    rdfs:range sd:NamedGraph .

kgi:endpointURLextension rdf:type owl:DatatypeProperty ;
    rdfs:label "Domain extension of the endpoint's URL"
    rdfs:domain dcat:DataService , sd:Service ;
    rdfs:range xsd:string .

# Description of the common elements between datasets

kgi:CommonalityDescription rdf:type rdfs:Class ;
    rdfs:label "Commonality description"@en;
    rdfs:comment "Description of the common elements between datasets. This is designed to give the elements that are common between the descriptions of groups of datasets"@en;
    rdfs:isDefinedBy kgi: .

kgi:dataset rdf:type owl:ObjectProperty ;
    rdfs:label "dataset"@en ;
    rdf:comment "Indicates the datasets whose commonalities are described by an instance of CommonalityDescription."@en ;
    rdfs:domain kgi:CommonalityDescription ;
    rdfs:range void:Dataset ;
    rdfs:isDefinedBy kgi: .

kgi:VocabularyCommonalityDescription rdf:subClassOf kgi:CommonalityDescription ;
    rdfs:label "Common vocabularies"@en ;
    rdfs:comment "Description of the common vocabularies used in different datasets. This shoud be used with kgi:vocabulary and kgi:dataset to list the vocabularies common to a group of datasets"@en ;
    rdfs:isDefinedBy kgi: .

kgi:ClassCommonalityDescription rdf:subClassOf kgi:CommonalityDescription ;
    rdfs:label "Common classes"@en ;
    rdfs:comment "Description of the common classes used in different datasets. This shoud be used with kgi:class and kgi:dataset to list the classes common to a group of datasets"@en ;
    rdfs:isDefinedBy kgi: .

kgi:PropertyCommonalityDescription rdf:subClassOf kgi:CommonalityDescription ;
    rdfs:label "Common properties"@en ;
    rdfs:comment "Description of the common properties used in different datasets. This shoud be used with kgi:property and kgi:dataset to list the properties common to a group of datasets"@en ;
    rdfs:isDefinedBy kgi: .

kgi:DatatypeCommonalityDescription rdf:subClassOf kgi:CommonalityDescription ;
