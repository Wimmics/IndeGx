# Annotations

kgi:passed rdf:type owl:AnnotationProperty ;
    rdfs:subPropertyOf kgi:tested ;
    rdfs:label "passed"@en ;
    rdfs:comment "Annotation to indicate the success of a test or action."@en ;
    rdfs:isDefinedBy kgi: ;
    rdfs:range earl:ManifestEntry , earl:TestCase .

kgi:failed rdf:type owl:AnnotationProperty ;
    rdfs:subPropertyOf kgi:tested ;
    rdfs:label "failed"@en ;
    rdfs:comment "Annotation to indicate the failure of a test or action."@en ;
    rdfs:isDefinedBy kgi: ;
    rdfs:range earl:ManifestEntry , earl:TestCase .

kgi:tested rdf:type owl:AnnotationProperty ;
    rdfs:label "tested"@en ;
    rdfs:comment "Annotation to indicate that a test or action has been executed."@en ;
    rdfs:isDefinedBy kgi: ;
    rdfs:range earl:ManifestEntry , earl:TestCase ;
    owl:propertyChainAxiom ( kgi:subjectOfAssertion earl:test ) .

kgi:subjectOfAssertion rdf:type owl:AnnotationProperty ;
    rdfs:label "subject of assertion"@en ;
    rdfs:comment "Annotation to indicate the subject of an assertion."@en ;
    rdfs:isDefinedBy kgi: ;
    rdfs:range earl:Assertion ;
    rdfs:domain earl:TestSubject ;
    owl:inverseOf earl:subject .

## Availability of an endpoint

kgi:AvailableEndpoint rdf:type owl:Class ;
    rdfs:label "Available endpoint"@en ;
    rdfs:comment "Annotation to indicate the availability of an endpoint at the moment of the last indexing."@en ;
    rdfs:isDefinedBy kgi: ;
    rdfs:subClassOf dcat:DataService , sd:Service ;
    owl:equivalentClass [
        rdf:type owl:Restriction ;
        owl:onProperty kgi:subjectOfAssertion ;
        owl:allValuesFrom [
            rdf:type owl:Class ;
            owl:intersectionOf ( 
                [
                    rdf:type owl:Restriction ;
                    owl:onProperty earl:result ;
                    owl:someValuesFrom [
                        rdf:type owl:Restriction ;
                        owl:onProperty earl:outcome ;
                        owl:hasValue earl:passed
                    ]
                ]
                [
                    rdf:type owl:Restriction ;
                    owl:onProperty earl:test ;
                    owl:hasValue <https://raw.githubusercontent.com/Wimmics/IndeGx/intentionDeclaration/rules/intension/main/online.ttl>
                ]
            )
        ]
    ] ,
    [ 
        rdf:type owl:Restriction ;
        owl:onProperty kgi:passed ;
        owl:hasValue <https://raw.githubusercontent.com/Wimmics/IndeGx/intentionDeclaration/rules/intension/main/online.ttl>
    ] .

kgi:UnavailableEndpoint rdf:type owl:Class ;
    rdfs:subClassOf dcat:DataService ;
    rdfs:comment "Annotation to indicate the unavailability of an endpoint."@en ;
    rdfs:isDefinedBy kgi: ;
    rdfs:label "Unavailable endpoint"@en ;
    owl:equivalentClass [
        rdf:type owl:Restriction ;
        owl:onProperty kgi:subjectOfAssertion ;
        owl:allValuesFrom [
            rdf:type owl:Class ;
            owl:intersectionOf ( 
                [
                    rdf:type owl:Restriction ;
                    owl:onProperty earl:result ;
                    owl:someValuesFrom [
                        rdf:type owl:Restriction ;
                        owl:onProperty earl:outcome ;
                        owl:hasValue earl:failed
                    ]
                ]
                [
                    rdf:type owl:Restriction ;
                    owl:onProperty earl:test ;
                    owl:hasValue <https://raw.githubusercontent.com/Wimmics/IndeGx/intentionDeclaration/rules/intension/main/online.ttl>
                ]
            )
        ]
    ] ,
    [ 
        rdf:type owl:Restriction ;
        owl:onProperty kgi:failed ;
        owl:hasValue <https://raw.githubusercontent.com/Wimmics/IndeGx/intentionDeclaration/rules/intension/main/online.ttl>
    ] .


# Capacities of the endpoint

kgi:SPARQL10Compliant rdf:type owl:Class ;
    rdfs:label "SPARQL 1.0 compliant endpoint"@en ;
    rdfs:comment "Annotation to indicate that an endpoint is compliant with SPARQL 1.0."@en ;
    rdfs:isDefinedBy kgi: ;
    rdfs:subClassOf sd:Service , dcat:DataService ;
    owl:equivalentClass [
        rdf:type owl:Restriction ;
        owl:onProperty sd:feature ;
        owl:hasValue sd:SPARQL10Query
    ] ,
    [
        rdf:type owl:Class ;
        owl:intersectionOf (
            [
                rdf:type owl:Restriction ;
                owl:onProperty sd:feature ;
                owl:hasValue sparqles:ASK
            ]
            [
                rdf:type owl:Restriction ;
                owl:onProperty sd:feature ;
                owl:hasValue sparqles:CON
            ]
            [
                rdf:type owl:Restriction ;
                owl:onProperty sd:feature ;
                owl:hasValue sparqles:CONJOIN
            ]
            [
                rdf:type owl:Restriction ;
                owl:onProperty sd:feature ;
                owl:hasValue sparqles:CONOPT
            ]
            [
                rdf:type owl:Restriction ;
                owl:onProperty sd:feature ;
                owl:hasValue sparqles:SEL-DISTINCT
            ]
            [
                rdf:type owl:Restriction ;
                owl:onProperty sd:feature ;
                owl:hasValue sparqles:SEL-REDUCED
            ]
            [
                rdf:type owl:Restriction ;
                owl:onProperty sd:feature ;
                owl:hasValue sparqles:SEL
            ]
            [
                rdf:type owl:Restriction ;
                owl:onProperty sd:feature ;
                owl:hasValue sparqles:SELBNODE
            ]
            [
                rdf:type owl:Restriction ;
                owl:onProperty sd:feature ;
                owl:hasValue sparqles:SELEMPTY
            ]
            [
                rdf:type owl:Restriction ;
                owl:onProperty sd:feature ;
