            ]
            [
                rdf:type owl:Restriction ;
                owl:onProperty sd:feature ;
                owl:hasValue sparqles:SELFILNOTBOUND
            ]
            [
                rdf:type owl:Restriction ;
                owl:onProperty sd:feature ;
                owl:hasValue sparqles:SELFILNUM
            ]
            [
                rdf:type owl:Restriction ;
                owl:onProperty sd:feature ;
                owl:hasValue sparqles:SELFILREGEX-i
            ]
            [
                rdf:type owl:Restriction ;
