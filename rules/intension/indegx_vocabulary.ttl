            ]
            [
                rdf:type owl:Restriction ;
                owl:onProperty sd:feature ;
                owl:hasValue sparqles:SELGRAPHJOIN
            ]
            [
                rdf:type owl:Restriction ;
                owl:onProperty sd:feature ;
