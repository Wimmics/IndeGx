kgi:DatasetWithAccessMeansAnnotations rdf:type owl:Class ;
    rdfs:label "Dataset that contains a description of the means of access of the dataset"@en ;
    rdfs:isDefinedBy kgi: ;
    # { ?kg void:sparqlEndpoint ?endpoint . }
    # UNION { ?kg void:dataDump ?dataDump . }
    # UNION { ?kg dcat:distribution ?dist .
    #     dist dataid:accessProcedure ?procedure . }
    owl:equivalentClass 
    [
        rdf:type owl:Class ;
        owl:unionOf (
            kgi:DatasetWithSparqlEndpointAnnotation
            kgi:DatasetWithDumpAnnotation
            [
                rdf:type owl:Class ;
                owl:intersectionOf (
                    [
                        rdf:type owl:Class ;
                        owl:unionOf (
                            dcat:Dataset
                            schema:Dataset
                            dcmitype:Dataset
                            void:Dataset
                            sd:Dataset
                        )
                    ]
                    [
                        rdf:type owl:Class ;
                        owl:unionOf (
                            [
                                rdf:type owl:Restriction ;
                                owl:onProperty dcat:distribution ;
                                owl:someValuesFrom owl:Thing
                            ]
                            [
                                rdf:type owl:Restriction ;
                                owl:onProperty dcat:distribution ;
                                owl:someValuesFrom [
                                    [
                                        rdf:type owl:Restriction ;
                                        owl:onProperty dataid:accessProcedure ;
                                        owl:someValuesFrom rdfs:Resource
                                    ]
                                ]
                            ]
                        )
                    ] 
                )
            ]
        )
    ] .



kgi:DatasetWithSparqlEndpointAnnotation rdf:type owl:Class ;
    rdfs:label "Dataset that contains a description of the address of the endpoint of the dataset"@en ;
    rdfs:isDefinedBy kgi: ;
    # { ?kg void:sparqlEndpoint ?endpoint . }
    # UNION { ?kg dcat:distribution ?dist .
    #     ?dist dcterms:format "application/sparql-query" .
    #     ?dist dcat:accessURL ?endpoint . }
    owl:equivalentClass [
        rdf:type owl:Class ;
        owl:intersectionOf (
            [
                rdf:type owl:Class ;
                owl:unionOf (
                    dcat:Dataset
                    schema:Dataset
                    dcmitype:Dataset
                    void:Dataset
                    sd:Dataset
                )
            ]
            [
                rdf:type owl:Class ;
                owl:unionOf (
                    [
                        rdf:type owl:Restriction ;
                        owl:onProperty void:sparqlEndpoint ;
                        owl:someValuesFrom rdfs:Resource
                    ]
                    [
                        rdf:type owl:Restriction ;
                        owl:onProperty dcat:distribution ;
                        owl:someValuesFrom [
                            rdf:type owl:Class ;
                            owl:intersectionOf (
                                [
                                    rdf:type owl:Restriction ;
                                    owl:onProperty dcterms:format ;
                                    owl:hasValue "application/sparql-query"
                                ]
                                [
                                    rdf:type owl:Restriction ;
                                    owl:onProperty dcat:accessURL ;
                                    owl:someValuesFrom rdfs:Resource
                                ]
                            )
                        ]
                    ]
                )
            ] 
        )
    ] .

