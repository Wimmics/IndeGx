## Annotation classes of the lowest level (without direct correspondance with concepts from Accountability)

kgi:DatasetWithDescriptionAnnotation rdf:type owl:Class ;
    rdfs:label "Dataset that contains a description of the description of the dataset"@en ;
    rdfs:isDefinedBy kgi: ;
    # ?kg dcterms:description ?desc .
    owl:equivalentClass [ 
        rdf:type owl:Class ;
        owl:intersectionOf (
            [
                rdf:type owl:Class ;
                owl:unionOf (
                    dcat:Dataset
                    schema:Dataset
                    dcmitype:Dataset
                    void:Dataset
                    sd:Dataset
                )
            ]
            [
                rdf:type owl:Class ;
                owl:unionOf (
                    [
                        rdf:type owl:Restriction ;
                        owl:onProperty dcterms:description ;
                        owl:someValuesFrom rdfs:Resource
                    ]
                )
            ]
        )
    ] .

