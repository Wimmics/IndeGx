kgi:DatasetWithCreationMethodAnnotation rdf:type owl:Class ;
    rdfs:label "Dataset that contains a description of the method of the creation of the dataset"@en ;
    rdfs:isDefinedBy kgi: ;
    # Equivalence to the IndeGx query rule version:
    # { ?kg dcterms:accrualMethod ?meth . }
    # UNION { ?kg pav:createdWith ?agent . }
    owl:equivalentClass [
        rdf:type owl:Class ;
        owl:intersectionOf (
            [
                rdf:type owl:Class ;
                owl:unionOf (
                    dcat:Dataset
                    schema:Dataset
                    dcmitype:Dataset
                    void:Dataset
                    sd:Dataset
                )
            ]
            [
                rdf:type owl:Class ;
                owl:unionOf (
                    [
                        rdf:type owl:Restriction ;
                        owl:onProperty dcterms:accrualMethod ;
                        owl:someValuesFrom owl:Thing
                    ]
                    [
