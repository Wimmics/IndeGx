## Accountability
# https://github.com/Jendersen/KG_accountability

kgi:Accountability rdf:type dqv:Category ;
    rdfs:label "Accountability Category"@en .

## Definition of metrics using DQV
### Creation metrics

kgi:datasetCreationAccountablity rdf:type dqv:Dimension ;
    rdfs:label "Dataset creation accountability dimension"@en ;
    rdf:comment "Annotation to indicate that the information related to the creation of a dataset are given in the dataset description."@en ;
    dqv:inCategory kgi:AccountabilityDimension .

kgi:datasetCreationAccountablity_Who rdf:type dqv:Dimension ;
    rdfs:label "Dataset creation accountable: who"@en ;
    rdfs:comment "Annotation to indicate that the information related to the creators of a dataset are given in the dataset description."@en ;
    dqv:inDimension kgi:datasetCreationAccountablity .

kgi:datasetCreationContributor rdf:type dqv:Metric ;
    dqv:expectedDataType xsd:boolean ;    
    dqv:inDimension kgi:datasetCreationAccountablity_Who ;
    dcterms:title "Creation contributor" ;
    dcterms:description "Who are the creators and contributors of the dataset? What is their role in the data creation process?" ;
    dqv:inCategory kgi:AccountabilityDimension .

kgi:datasetCreationAccountablity_When rdf:type dqv:Dimension ;
    rdfs:label "Dataset creation accountable: when"@en ;
    rdfs:comment "Annotation to indicate that the information related to the time of the creation of a dataset are given in the dataset description."@en ;
    dqv:inDimension kgi:datasetCreationAccountablity .

kgi:datasetCreationDate rdf:type dqv:Metric ;
