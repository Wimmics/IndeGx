## Type of datasets according to the elements in their descriptions

kgi:AccountableDataset rdf:type owl:Class ;
    rdfs:label "Accountable dataset"@en ;
    rdfs:subClassOf dcat:Dataset ,
        schema:Dataset ,
        dcmitype:Dataset , 
        void:Dataset ,
        sd:Dataset ;
    owl:equivalentClass 
    # Straightforward definition of accountable dataset according to its 3 dimensions
    [
        rdf:type owl:Class ;
        owl:intersectionOf (
            kgi:DatasetWithCreationAccountability
            kgi:DatasetWithMaintenanceAccountability
            kgi:DatasetWithUsageAccountability
        )
    ] ,
    # Definition of accountable dataset according to the 5 questions
    [
        rdf:type owl:Class ;
        owl:intersectionOf (
            kgi:DatasetWithWhoQuestionAccountability
            kgi:DatasetWithWhenQuestionAccountability
            kgi:DatasetWithWhereQuestionAccountability
            kgi:DatasetWithHowQuestionAccountability
            kgi:DatasetWithWhatQuestionAccountability
        )
    ] .

kgi:DatasetWithCreationAccountability rdf:type owl:Class ;
    rdfs:label "Dataset with creation accountability"@en ;
    owl:equivalentClass [
        rdf:type owl:Class ;
        owl:intersectionOf (
            kgi:DatasetWithCreationWhoAccountability
            kgi:DatasetWithCreationWhenAccountability
            kgi:DatasetWithCreationWhereAccountability
            kgi:DatasetWithCreationHowAccountability
        )
    ] .

kgi:DatasetWithMaintenanceAccountability rdf:type owl:Class ;
    rdfs:label "Dataset with maintenance accountability"@en ;
    owl:equivalentClass [
        rdf:type owl:Class ;
        owl:intersectionOf (
