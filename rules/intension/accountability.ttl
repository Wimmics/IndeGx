kgi:DatasetWithWhatQuestionAccountability rdf:type owl:Class ;
    rdfs:label "Dataset with what question accountable"@en ;
    owl:equivalentClass [
        rdf:type owl:Class ;
        owl:intersectionOf (
            kgi:DatasetWithUsageWhatAccountability
        )
    ] .

kgi:DatasetWithCreationWhoAccountability rdf:type owl:Class ;
    rdfs:label "Dataset with creation accountability - Who ?"@en ;
    owl:equivalentClass [
        rdf:type owl:Class ;
        owl:intersectionOf (
            kgi:DatasetWithContributorAnnotation
            kgi:DatasetWithAuthorAnnotation
        )
    ] ,
    # Equivalence with having a successful quality measurement of the creation/Who/contributor metric
    # ?kg dqv:hasQualityMeasurement ?qualityMeasurementContributor .
    # ?qualityMeasurementContributor a dqv:QualityMeasurement ;
    #     rdfs:label "Creation contributor measurement" ;
    #     dqv:isMeasurementOf kgi:datasetCreationContributor ;
    #     dqv:value "true"^^xsd:boolean .
    [
        rdf:type owl:Restriction ;
        owl:onProperty dqv:hasQualityMeasurement ;
        owl:someValuesFrom [
            rdf:type owl:Class ;
            owl:intersectionOf (
                [
                    rdf:type owl:Restriction ;
                    owl:onProperty dqv:isMeasurementOf ;
                    owl:hasValue kgi:datasetCreationContributor
                ]
                [
                    rdf:type owl:Restriction ;
                    owl:onProperty dqv:value ;
                    owl:hasValue "true"^^xsd:boolean
                ]
            ) 
        ]
    ] .

kgi:DatasetWithCreationWhenAccountability rdf:type owl:Class ;
    rdfs:label "Dataset with creation accountability - When ?"@en ;
    owl:equivalentClass kgi:DatasetWithCreationDateAnnotation ,
    # Equivalence with having a successful quality measurement of the creation/When metric
    # ?kg dqv:hasQualityMeasurement ?qualityMeasurementDate .
    # ?qualityMeasurementDate a dqv:QualityMeasurement ;
    #     rdfs:label "Creation date measurement" ;
    #     dqv:isMeasurementOf kgi:datasetCreationDate ;
    #     dqv:value "true"^^xsd:boolean .
    [
        rdf:type owl:Restriction ;
        owl:onProperty dqv:hasQualityMeasurement ;
        owl:someValuesFrom [
            rdf:type owl:Class ;
            owl:intersectionOf (
                [
                    rdf:type owl:Restriction ;
                    owl:onProperty dqv:isMeasurementOf ;
                    owl:hasValue kgi:datasetCreationDate
                ]
                [
                    rdf:type owl:Restriction ;
                    owl:onProperty dqv:value ;
                    owl:hasValue "true"^^xsd:boolean
                ]
            )
        ]
    ] .

kgi:DatasetWithCreationWhereAccountability rdf:type owl:Class ;
    rdfs:label "Dataset with creation accountability - Where ?"@en ;
    owl:equivalentClass [
        rdf:type owl:Class ;
        owl:unionOf (
            kgi:DatasetWithCreationSourceAccountability
            kgi:DatasetWithCreationLocationAccountability
        )
    ] .

kgi:DatasetWithCreationHowAccountability rdf:type owl:Class ;
    rdfs:label "Dataset with creation accountability - How ?"@en ;
    owl:equivalentClass kgi:DatasetWithCreationMethodologyAccountability .

kgi:DatasetWithMaintenanceWhoAccountability rdf:type owl:Class ;
    rdfs:label "Dataset with maintenance accountability - Who ?"@en ;
    owl:equivalentClass kgi:DatasetWithMaintenanceContributorAccountability .

kgi:DatasetWithMaintenanceWhenAccountability rdf:type owl:Class ;
    rdfs:label "Dataset with maintenance accountability - When ?"@en ;
    owl:equivalentClass [
        rdf:type owl:Class ;
        owl:intersectionOf (
            kgi:DatasetWithModificationAnnotation
            kgi:DatasetWithCurationAnnotation
            kgi:DatasetWithAccrualFrequencyAnnotation
