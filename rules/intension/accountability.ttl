            kgi:DatasetWithMaintenanceWhenAccountability
            kgi:DatasetWithMaintenanceWhereAccountability
            kgi:DatasetWithMaintenanceHowAccountability
        )
    ] .

kgi:DatasetWithUsageAccountability rdf:type owl:Class ;
    rdfs:label "Dataset with usage accountability"@en ;
    owl:equivalentClass [
        rdf:type owl:Class ;
        owl:intersectionOf (
