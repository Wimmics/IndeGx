    dqv:inCategory kgi:AccountabilityDimension .

kgi:datasetCreationAccountablity_Where rdf:type dqv:Dimension ;
    rdfs:label "Dataset creation accountable: where"@en ;
    rdfs:comment "Annotation to indicate that the information related to the location of the creation of a dataset are given in the dataset description."@en ;
    dqv:inDimension kgi:datasetCreationAccountablity .

kgi:datasetCreationSource rdf:type dqv:Metric ;
    dqv:expectedDataType xsd:boolean ;    
