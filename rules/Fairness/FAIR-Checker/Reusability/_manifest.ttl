@prefix mf:      <http://www.w3.org/2001/sw/DataAccess/tests/test-manifest#> .
@prefix kgi: <http://ns.inria.fr/kg/index#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<> a mf:Manifest ;
    mf:entries (
        <R11.ttl>
        <R12.ttl>
        <R13.ttl>
    ) .

<R11.ttl> a mf:ManifestEntry ;
    kgi:onSuccess (
        [
            dcterms:description "At least one of the discoverability properties was found in metadata" ;
            mf:action """
                PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
                PREFIX nie: <http://www.semanticdesktop.org/ontologies/2007/01/19/nie#>
                PREFIX sto: <https://w3id.org/i40/sto#>
                PREFIX xhv: <http://www.w3.org/1999/xhtml/vocab#>
                PREFIX cc: <http://creativecommons.org/ns#>
                PREFIX dbpo: <http://dbpedia.org/ontology/>
                PREFIX dbp: <http://dbpedia.org/property/>
                PREFIX dbpedia: <http://dbpedia.org/resource/>
                PREFIX doap: <http://usefulinc.com/ns/doap#>
                PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
                PREFIX void: <http://rdfs.org/ns/void#>
                PREFIX dcat: <http://www.w3.org/ns/dcat#>
                PREFIX dcmitype: <http://purl.org/dc/dcmitype/>
                PREFIX schema: <http://schema.org/>
                PREFIX sd: <http://www.w3.org/ns/sparql-service-description#>
                PREFIX prov: <http://www.w3.org/ns/prov#>
                PREFIX pav: <http://purl.org/pav/>
                PREFIX foaf: <http://xmlns.com/foaf/0.1/>
                PREFIX dqv: <http://www.w3.org/ns/dqv#>
                PREFIX dce: <http://purl.org/dc/elements/1.1/>
                PREFIX dcterms: <http://purl.org/dc/terms/>
                PREFIX dataid: <http://dataid.dbpedia.org/ns/core#>
                PREFIX kgi: <http://ns.inria.fr/kg/index#>
                INSERT {
                    GRAPH ?kgGraph {
                        ?kg dqv:hasQualityMeasurement ?measurement .
                        ?measurement a dqv:QualityMeasurement ;
                            rdfs:comment ?comment ;
                            rdfs:label "Metadata includes license"@en ;
                            dqv:isMeasurementOf <https://w3id.org/fair/principles/latest/R1#R11> ;
                            dqv:value ?value .
                    }
                } WHERE {
                    kgi:FAIRness kgi:dataGraph ?kgGraph .
                    GRAPH ?kgGraph {
                        { ?kg a dcat:Dataset }
                        UNION { ?kg a void:Dataset }
                        UNION { ?kg a dcmitype:Dataset }
                        UNION { ?kg a schema:Dataset }
                        UNION { ?kg a sd:Dataset }
                        UNION { ?kg a dataid:Dataset }
                    }
                    BIND( IF (EXISTS {
                        GRAPH ?kgGraph {
                            ?kg ?p ?o . 
                        }
                        VALUES ?p {  
                            schema:license 
                            dcterms:license 
                            doap:license 
                            dbpo:license 
                            cc:license 
                            xhv:license 
                            sto:license 
                            nie:license 
                        }
                        FILTER(! isBlank(?o))
                    }, 2, 0) AS ?value )
                    OPTIONAL {
                        GRAPH ?kgGraph {
                            ?kg ?p ?o . 
                        }
                        VALUES ?p {  
                            schema:license 
                            dcterms:license 
                            doap:license 
                            dbpo:license 
                            cc:license 
                            xhv:license 
                            sto:license 
                            nie:license 
                        }
                        FILTER(! isBlank(?o))
                        BIND(CONCAT("License found: ", STR(?o)) AS ?comment)
                    }
                    BIND(Iri(CONCAT(STR(?kg), "#R11")) AS ?measurement)
                }
                """
        ]
    ) .


<R12.ttl> a mf:ManifestEntry ;
    kgi:onSuccess (
        [
            dcterms:description "At least one of the discoverability properties was found in metadata" ;
            mf:action """
                PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
                PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
                PREFIX void: <http://rdfs.org/ns/void#>
                PREFIX dcat: <http://www.w3.org/ns/dcat#>
                PREFIX dcmitype: <http://purl.org/dc/dcmitype/>
                PREFIX schema: <http://schema.org/>
                PREFIX sd: <http://www.w3.org/ns/sparql-service-description#>
                PREFIX prov: <http://www.w3.org/ns/prov#>
                PREFIX pav: <http://purl.org/pav/>
                PREFIX foaf: <http://xmlns.com/foaf/0.1/>
                PREFIX dqv: <http://www.w3.org/ns/dqv#>
                PREFIX dce: <http://purl.org/dc/elements/1.1/>
                PREFIX dcterms: <http://purl.org/dc/terms/>
                PREFIX dataid: <http://dataid.dbpedia.org/ns/core#>
                PREFIX kgi: <http://ns.inria.fr/kg/index#>
                INSERT {
                    GRAPH ?kgGraph {
                        ?kg dqv:hasQualityMeasurement ?measurement .
                        ?measurement a dqv:QualityMeasurement ;
                            rdfs:label "Metadata includes provenance"@en ;
                            rdfs:comment ?comment ;
                            dqv:isMeasurementOf <https://w3id.org/fair/principles/latest/R1#R12> ;
                            dqv:value ?value .
                    }
                } WHERE {
                    kgi:FAIRness kgi:dataGraph ?kgGraph .
                    GRAPH ?kgGraph {
                        { ?kg a dcat:Dataset }
                        UNION { ?kg a void:Dataset }
                        UNION { ?kg a dcmitype:Dataset }
                        UNION { ?kg a schema:Dataset }
                        UNION { ?kg a sd:Dataset }
                        UNION { ?kg a dataid:Dataset }
                    }
                    BIND( IF (EXISTS {
                        GRAPH ?kgGraph {
                            ?kg ?p ?o . 
                        }
                        VALUES ?p {  
                            prov:wasGeneratedBy 
                            prov:wasDerivedFrom 
                            prov:wasAttributedTo 
                            prov:used 
                            prov:wasInformedBy 
                            prov:wasAssociatedWith
                            prov:startedAtTime 
                            prov:endedAtTime
                            dcterms:hasVersion
                            dcterms:isVersionOf 
                            dcterms:creator 
                            dcterms:contributor 
                            dcterms:publisher
                            pav:hasVersion
                            pav:version 
                            pav:hasCurrentVersion 
                            pav:createdBy 
                            pav:authoredBy 
                            pav:retrievedFrom 
                            pav:importedFrom 
                            pav:createdWith 
                            pav:retrievedBy 
                            pav:importedBy 
                            pav:curatedBy 
                            pav:createdAt 
                            pav:previousVersion
                            schema:creator
                            schema:author
                            schema:publisher
                            schema:provider
                            schema:funder
                        }
                    }, 2, 0) AS ?value )
                    OPTIONAL {
                        GRAPH ?kgGraph {
                            ?kg ?p ?o . 
                        }
                        VALUES ?p {  
                            prov:wasGeneratedBy 
                            prov:wasDerivedFrom 
                            prov:wasAttributedTo 
                            prov:used 
                            prov:wasInformedBy 
                            prov:wasAssociatedWith
                            prov:startedAtTime 
                            prov:endedAtTime
                            dcterms:hasVersion
                            dcterms:isVersionOf 
                            dcterms:creator 
                            dcterms:contributor 
                            dcterms:publisher
                            pav:hasVersion
                            pav:version 
                            pav:hasCurrentVersion 
                            pav:createdBy 
                            pav:authoredBy 
                            pav:retrievedFrom 
                            pav:importedFrom 
                            pav:createdWith 
                            pav:retrievedBy 
                            pav:importedBy 
                            pav:curatedBy 
                            pav:createdAt 
                            pav:previousVersion
                            schema:creator
                            schema:author
                            schema:publisher
                            schema:provider
                            schema:funder
                        }
                        BIND(CONCAT("Provenance found: ", STR(?p)) AS ?comment)
                    }
                    BIND(Iri(CONCAT(STR(?kg), "#R12")) AS ?measurement)
                }
                """
        ]
    ) .

<R13.ttl> a mf:Manifest ;
    kgi:requiredAssets (
        <../Findability/F2B.ttl>
    ) ;
    kgi:onSuccess (
        [
            mf:action """
                PREFIX void: <http://rdfs.org/ns/void#>
                PREFIX dcat: <http://www.w3.org/ns/dcat#>
                PREFIX dcmitype: <http://purl.org/dc/dcmitype/>
                PREFIX schema: <http://schema.org/>
                PREFIX sd: <http://www.w3.org/ns/sparql-service-description#>
                PREFIX prov: <http://www.w3.org/ns/prov#>
                PREFIX pav: <http://purl.org/pav/>
                PREFIX foaf: <http://xmlns.com/foaf/0.1/>
                PREFIX dqv: <http://www.w3.org/ns/dqv#>
                PREFIX dce: <http://purl.org/dc/elements/1.1/>
                PREFIX dcterms: <http://purl.org/dc/terms/>
                PREFIX dataid: <http://dataid.dbpedia.org/ns/core#>
                PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
                PREFIX kgi: <http://ns.inria.fr/kg/index#>
                INSERT {
                    GRAPH ?kgGraph {
                        ?kg dqv:hasQualityMeasurement ?measurement .
                        ?measurement a dqv:QualityMeasurement ;
                            rdfs:label "Community standards"@en ;
                            dqv:isMeasurementOf <https://w3id.org/fair/principles/latest/R1#R13> ;
                            dqv:value ?value .
                    }
                } WHERE {
                    kgi:FAIRness kgi:dataGraph ?kgGraph .
                    GRAPH ?kgGraph {
                        ?kg dqv:hasQualityMeasurement ?F2Bmeasurement .
                        ?F2Bmeasurement a dqv:QualityMeasurement ;
                            dqv:isMeasurementOf <https://w3id.org/fair/principles/latest/F2#F2B> ;
                            dqv:value ?value .
                    }
                    BIND(Iri(CONCAT(STR(?kg), "#R13")) AS ?measurement)
                }
            """
        ]
    ) .