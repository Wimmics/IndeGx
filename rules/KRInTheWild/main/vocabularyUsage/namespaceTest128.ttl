@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix earl: <http://www.w3.org/ns/earl#> .
@prefix kgi: <http://ns.inria.fr/kg/index#> .
@prefix prov: <http://www.w3.org/ns/prov#> .

<> rdf:type kgi:TestQuery ;
    dcterms:isReferencedBy <manifest.ttl> ;
    dcterms:title "Text of the presence of class <http://purl.oclc.org/NET/UNIS/fiware/iot-lite>" ;
    kgi:query """PREFIX voaf: <http://purl.org/vocommons/voaf#>
    PREFIX kgi: <http://ns.inria.fr/kg/index#>
    PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
    PREFIX prov: <http://www.w3.org/ns/prov#>
    PREFIX dcat: <http://www.w3.org/ns/dcat#>
    PREFIX void: <http://rdfs.org/ns/void#>
    PREFIX owl: <http://www.w3.org/2002/07/owl#>
    PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
    PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
    PREFIX sd: <http://www.w3.org/ns/sparql-service-description#>
    PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
    ASK { 
        kgi: sd:namedGraph $rawEndpointUrl .
        BIND( IRI( CONCAT( STR($rawEndpointUrl), "?limit=1" ) ) AS ?editedEndpointUrl )
        SERVICE ?editedEndpointUrl {
            {
                SELECT ?whatever WHERE {
                    ?s a ?whatever .
                    FILTER( STRSTARTS( STR(?whatever), STR(<http://purl.oclc.org/NET/UNIS/fiware/iot-lite>) ) )
                } LIMIT 1
            }
        }
    }""" ,
    """PREFIX voaf: <http://purl.org/vocommons/voaf#>
    PREFIX kgi: <http://ns.inria.fr/kg/index#>
    PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
    PREFIX prov: <http://www.w3.org/ns/prov#>
    PREFIX dcat: <http://www.w3.org/ns/dcat#>
    PREFIX void: <http://rdfs.org/ns/void#>
    PREFIX owl: <http://www.w3.org/2002/07/owl#>
    PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
    PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
    PREFIX sd: <http://www.w3.org/ns/sparql-service-description#>
    PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
    ASK { 
        kgi: sd:namedGraph $rawEndpointUrl .
        BIND( IRI( CONCAT( STR($rawEndpointUrl), "?limit=1" ) ) AS ?editedEndpointUrl )
        SERVICE ?editedEndpointUrl {
            {
                SELECT ?whatever WHERE {
                    ?s ?whatever ?o .
                    FILTER( STRSTARTS( STR(?whatever), STR(<http://purl.oclc.org/NET/UNIS/fiware/iot-lite>) ) )
                } LIMIT 1
            }
        }
    }""" .