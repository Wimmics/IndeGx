@prefix kgi: <http://ns.inria.fr/kg/index#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<> a kgi:TestQuery ;
    dcterms:title "Presence of an alternative title." ;
    dcterms:isReferencedBy <_manifest.ttl> ;
    dcterms:description "Look for an alternative title of the KG." ;
    kgi:query """
        PREFIX void: <http://rdfs.org/ns/void#>
        PREFIX dcat: <http://www.w3.org/ns/dcat#>
        PREFIX dcmitype: <http://purl.org/dc/dcmitype/>
        PREFIX schema: <http://schema.org/>
        PREFIX sd: <http://www.w3.org/ns/sparql-service-description#>
        PREFIX dcterms: <http://purl.org/dc/terms/>
        PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
        ASK {
            { ?res schema:alternateName ?id }
            UNION { ?res dcterms:alternative ?id }
            UNION { ?res skos:altLabel ?id }
        }
""" .
