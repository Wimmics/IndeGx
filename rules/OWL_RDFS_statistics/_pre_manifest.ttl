@prefix kgi: <http://ns.inria.fr/kg/index#> .
@prefix mf: <http://www.w3.org/2001/sw/DataAccess/tests/test-manifest#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<> a mf:Manifest ;
    kgi:requiredAssets (
    ) ;
    mf:include (
    ) ;
    mf:entries (
        <loadVocabulary.ttl>
        <classes/classUsage.ttl>
        <properties/propertyUsage.ttl>
    ) .

<loadVocabulary.ttl> a mf:ManifestEntry ;
    kgi:onSuccess (
        [
            mf:action """
            LOAD <file:///input/owl.ttl> ;
            LOAD <file:///input/rdf-schema.ttl> 
            """
        ]
    ) .

<classes/classUsage.ttl> a mf:ManifestEntry ;
    kgi:requiredAssets (
        <loadVocabulary.ttl>
    ) ;
    kgi:onSuccess (
        [
            rdfs:label "Creating the basic description of each class" ;
            mf:action """PREFIX voaf: <http://purl.org/vocommons/voaf#>
            PREFIX kgi: <http://ns.inria.fr/kg/index#>
            PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
            PREFIX prov: <http://www.w3.org/ns/prov#>
            PREFIX dcat: <http://www.w3.org/ns/dcat#>
            PREFIX void: <http://rdfs.org/ns/void#>
            PREFIX owl: <http://www.w3.org/2002/07/owl#>
            PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
            PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
            PREFIX sd: <http://www.w3.org/ns/sparql-service-description#>
            PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

            INSERT {
                ?elem voaf:reusedByDatasets 0 .
            }
            WHERE {
                { ?elem a owl:Class }
                UNION
                { ?elem a rdfs:Class }
                FILTER(isIRI(?elem))
            }"""
        ]
    ) .

<properties/propertyUsage.ttl> a mf:ManifestEntry ;
    kgi:requiredAssets (
        <loadVocabulary.ttl>
    ) ;
    kgi:onSuccess (
        [
            rdfs:label "Creating the ontology element description of each property" ;
            mf:action """prefix voaf: <http://purl.org/vocommons/voaf#>
            prefix kgi: <http://ns.inria.fr/kg/index#>
            prefix xsd: <http://www.w3.org/2001/XMLSchema#>
            prefix prov: <http://www.w3.org/ns/prov#>
            prefix dcat: <http://www.w3.org/ns/dcat#>
            prefix void: <http://rdfs.org/ns/void#>
            prefix owl: <http://www.w3.org/2002/07/owl#>
            prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
            PREFIX sd: <http://www.w3.org/ns/sparql-service-description#>

            INSERT {
                ?elem voaf:reusedByDatasets 0 .
            }
            WHERE {
                { ?elem a rdf:Property }
                UNION { ?elem a owl:ObjectProperty }
                UNION { ?elem a owl:DatatypeProperty }
                UNION { ?elem a owl:AnnotationProperty }
                UNION { ?elem a owl:OntologyProperty }

                FILTER( isIRI(?elem) )
            }"""
        ]
    ) .