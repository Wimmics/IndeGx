@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix earl: <http://www.w3.org/ns/earl#> .
@prefix kgi: <http://ns.inria.fr/kg/index#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<> rdf:type kgi:TestQuery ;
    dcterms:isReferencedBy <manifest.ttl> ;
    dcterms:title "Presence of datatypes in the content." ;
    dcterms:description "Check if there are datatypes associated to literals." ;
    kgi:timeout "PT120S"^^xsd:duration ;
    kgi:query """ASK {
            SERVICE $rawEndpointUrl {
                SELECT DISTINCT (datatype(?o) as ?datatype) WHERE {
                    ?s ?p ?o . FILTER( isLiteral(?o) )
                } LIMIT 1
            }
        }""" .
