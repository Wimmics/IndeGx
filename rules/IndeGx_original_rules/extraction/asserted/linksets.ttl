@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix earl: <http://www.w3.org/ns/earl#> .
@prefix kgi: <http://ns.inria.fr/kg/index#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<> rdf:type kgi:TestQuery ;
    dcterms:title "Extraction of linksets" ;
    dcterms:isReferencedBy <manifest.ttl> ;
    dcterms:description "Extraction of the linksets defined in the dataset." ;
    kgi:query """PREFIX void: <http://rdfs.org/ns/void#>
        ASK {
            SERVICE $rawEndpointUrl {
                ?linkset a void:Linkset .
            }
        }""" .
