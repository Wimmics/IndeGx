@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix earl: <http://www.w3.org/ns/earl#> .
@prefix kgi: <http://ns.inria.fr/kg/index#> .

<> rdf:type kgi:TestQuery ;
    dcterms:isReferencedBy <manifest.ttl> ;
    dcterms:description "SPARQL 1.1 SELECT query containing a Property Path expression" ;
    kgi:query """SELECT * WHERE {
            SERVICE $rawEndpointUrl { 
                <http://nonsensical.com/1> <http://example.org/type>* ?z
            }
        }  LIMIT 10"""  .
