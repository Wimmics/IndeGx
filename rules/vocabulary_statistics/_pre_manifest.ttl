@prefix mf: <http://www.w3.org/2001/sw/DataAccess/tests/test-manifest#> .
@prefix kgi: <http://ns.inria.fr/kg/index#> .

<> a mf:Manifest ;
    kgi:requiredAssets (
    ) ;
    mf:include (
    ) ;
    mf:entries (
        <loadLOV.ttl>
        <classes/classUsage.ttl>
        <properties/propertyUsage.ttl>
    ) .

<loadLOV.ttl> a mf:ManifestEntry ;
    kgi:onSuccess (
        [
            mf:action """
            LOAD <https://raw.githubusercontent.com/Wimmics/IndeGx/VocabularyExperiment/data/LOV_redux.classes.ttl> ;
            LOAD <https://raw.githubusercontent.com/Wimmics/IndeGx/VocabularyExperiment/data/LOV_redux.properties.ttl> 
            """
        ]
    ) .

<classUsage.ttl> a mf:ManifestEntry ;
    kgi:requiredAssets (
        <loadLOV.ttl>
    ) ;
    kgi:onSuccess (
        [
            # Creating the basic description of each class
            mf:action """
            PREFIX voaf: <http://purl.org/vocommons/voaf#>
            PREFIX kgi: <http://ns.inria.fr/kg/index#>
            PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
            PREFIX prov: <http://www.w3.org/ns/prov#>
            PREFIX dcat: <http://www.w3.org/ns/dcat#>
            PREFIX void: <http://rdfs.org/ns/void#>
            PREFIX owl: <http://www.w3.org/2002/07/owl#>
            PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
            PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
            PREFIX sd: <http://www.w3.org/ns/sparql-service-description#>
            PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

            INSERT {
                ?elem voaf:reusedByDatasets 0 .
            }
            WHERE {
                ?elem a owl:Class .
                FILTER(isIRI(?elem))
            }
            """
        ]
    ) .

<propertyUsage.ttl> a mf:ManifestEntry ;
    kgi:requiredAssets (
        <loadLOV.ttl>
    ) ;
    kgi:onSuccess (
        [
            # Creating the ontology element description of each property
            mf:action """
            prefix voaf: <http://purl.org/vocommons/voaf#>
            prefix kgi: <http://ns.inria.fr/kg/index#>
            prefix xsd: <http://www.w3.org/2001/XMLSchema#>
            prefix prov: <http://www.w3.org/ns/prov#>
            prefix dcat: <http://www.w3.org/ns/dcat#>
            prefix void: <http://rdfs.org/ns/void#>
            prefix owl: <http://www.w3.org/2002/07/owl#>
            prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
            PREFIX sd: <http://www.w3.org/ns/sparql-service-description#>

            INSERT {
                ?elem voaf:reusedByDatasets 0 .
            }
            WHERE {
                ?elem a rdf:Property .
                FILTER( isIRI(?elem) )
            }
            """
        ]
    ) .