
%%%ENTRY%%% a mf:ManifestEntry ;
    kgi:requiredAssets (
        <init.ttl> 
    ) ;
    kgi:onSuccess (
        [
            rdfs:label "Creating the occurences resource of %%%namespace%%% in the dataset" ;
            mf:action """prefix voaf: <http://purl.org/vocommons/voaf#>
            prefix kgi: <http://ns.inria.fr/kg/index#>
            prefix xsd: <http://www.w3.org/2001/XMLSchema#>
            prefix prov: <http://www.w3.org/ns/prov#>
            prefix dcat: <http://www.w3.org/ns/dcat#>
            prefix void: <http://rdfs.org/ns/void#>
            prefix owl: <http://www.w3.org/2002/07/owl#>
            prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
            prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
            PREFIX sd: <http://www.w3.org/ns/sparql-service-description#>
            PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

            INSERT {
                %%%namespace%%% voaf:usageInDataset ?occurrence .
                ?occurrence a voaf:DatasetOccurences ;
                    voaf:inDataset $rawEndpointUrl .
            } WHERE {
                BIND(CONCAT( STR(kgi:), MD5( STR(?vocab) ) ) AS ?occurrence)
            }"""
        ]
    ) .