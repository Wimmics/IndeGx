@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix earl: <http://www.w3.org/ns/earl#> .
@prefix kgi: <http://ns.inria.fr/kg/index#> .
@prefix prov: <http://www.w3.org/ns/prov#> .

<> rdf:type kgi:TestQuery ;
    dcterms:isReferencedBy <manifest.ttl> ;
    dcterms:title "Text of the presence of property <http://www.w3.org/2008/05/skos-xl#altLabel>" ;
    kgi:query """ASK { 
            SERVICE $rawEndpointUrl { 
                {
                    SELECT ?s ?o WHERE { 
                        {
                            ?s <http://www.w3.org/2008/05/skos-xl#altLabel> ?o .
                        } UNION {
                            GRAPH ?g {
                                ?s <http://www.w3.org/2008/05/skos-xl#altLabel> ?o .
                            }
                        }
                    } LIMIT 1
                }
            } 
        }"""  .